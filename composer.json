{
    "name": "goldendeveloper/pinterest",
    "description": "Pinterest OAuth2 Provider for Laravel Socialite",
    "license": "MIT",
    "version": "1.0",
    "keywords": [
        "laravel",
        "oauth",
        "pinterest",
        "provider",
        "socialite"
    ],
    "authors": [
        {
            "name": "Collin Ilgner",
            "email": "cohohohn04@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "socialiteproviders/manager": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "GoldenDeveloper\\Pinterest\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/socialiteproviders/providers/issues",
        "source": "https://github.com/socialiteproviders/providers"
    },

    "extra": {
        "laravel": {
            "providers": [
                "GoldenDeveloper\\Pinterest\\PinterestServiceProvider"
            ]
        }
    }
}
